import java.util.*;
public class Main {
    static boolean checktarget(int[] arr, int v) {
        HashMap<Integer,Integer> map = new HashMap<>();
        for (int i = 0; i < arr.length; i++) {
            int complement = v - arr[i];
            if (map.containsKey(arr[i])) {
                return true;
            }
            map.put(complement, i);
        }
       return false;
    }
    public static void main(String[] args) {
        System.out.println("Enter the number of the values in the array");
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] arr = new int[n];
        for (int i = 0; i < n; i++) {
            System.out.println("Enter the number: " + (i + 1));
            arr[i] = sc.nextInt();
        }
        System.out.println(checktarget(arr, 6));
    }
}
