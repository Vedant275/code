import java.util.*;
public class Main {
   static void getTargetValues(int[] arr, int v) {
        HashMap<Integer, Boolean> map = new HashMap<>();
        for (int i = 0; i < arr.length; i++) {
            int complement = v - arr[i];
            if (map.containsKey(complement)) {
                System.out.println("Pair found: " + arr[i] + " + " + complement + " = " + v);
                return;
            }
            map.put(arr[i], true);
        }
        System.out.println("Pair not found");
    }
    public static void main(String[] args) {
        System.out.println("Enter the number of the values in the array");
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] arr = new int[n];
        for (int i = 0; i < n; i++) {
            System.out.println("Enter the number: " + (i + 1));
            arr[i] = sc.nextInt();
        }
        getTargetValues(arr, 6);
    }
}
