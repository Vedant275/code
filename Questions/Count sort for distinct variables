public class Main {
    public static void getcountSorted(int[] sample){
        int max = sample[0];
        for (int i = 0; i < sample.length; i++) {
            if (max < sample[i]) {
                max = sample[i];        // By doing these steps we founded the max number the array
            }
        }
        int[] count = new int[max + 1];
        for (int j = 0; j < sample.length; j++) {
            count[(sample[j] % 100)]++;          // By using this loop we just count the number of digits that appear in our unsorted array
        }
        int index = 0;
        for (int i = 0; i < count.length; i++) {
            while (count[i] > 0) {          // in this step if the number comes zero times it is skipped so sorted array is made
                sample[index++] = i;
                count[i]--;         // if a number comes 2 times and its printed 1 time it remains to be printed 1 time so substracted
            }
        }
        for (int j = 0; j < sample.length; j++) {
            System.out.println(sample[j]);     // the sorted array is printed
        }
    }
    public static void main(String[] args) {
        int[] sample = {4, 70, 54, 23, 45};
        getcountSorted(sample);
    }
}
